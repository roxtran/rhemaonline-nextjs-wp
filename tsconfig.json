{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/locations/ottawa.js", "pages/locations/toronto.js", "components/common/Modals/BibleModal.js", "components/common/Modals/ContactUsModal.js", "components/common/Modals/HomeModal.js", "pages/events.js", "pages/connect.js", "pages/beliefs.js", "pages/about.js", "components/common/HeadLine.js"  ],
  "exclude": [
    "node_modules"
  ]
}
